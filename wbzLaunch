.ToPILImage(),  # 将numpy数组转换为PIL图像\n    transforms.RandomRotation(30),  # 随机旋转，角度范围为-10到10度\n    transforms.ToTensor()  # 将PIL图像转换为Tensor\n])\n\n# 加载数据集并应用数据增强\ntrain_dataset = MNISTDataset('/kaggle/input/dataset/sign_mnist_train.csv', transform=transform)\ntrain_dataloader = DataLoader(train_dataset, batch_size=64, shuffle=True)\n\ntest_dataset = MNISTDataset('/kaggle/input/dataset/sign_mnist_test.csv')\ntest_dataloader = DataLoader(test_dataset, batch_size=64, shuffle=False)","metadata":{"execution":{"iopub.status.busy":"2024-07-18T08:12:07.244945Z","iopub.execute_input":"2024-07-18T08:12:07.245439Z","iopub.status.idle":"2024-07-18T08:12:11.381384Z","shell.execute_reply.started":"2024-07-18T08:12:07.245401Z","shell.execute_reply":"2024-07-18T08:12:11.380459Z"},"trusted":true},"execution_count":57,"outputs":[]},{"cell_type":"code","source":"class CNN(nn.Module):\n    def __init__(self):\n        super(CNN, self).__init__()\n        self.conv1 = nn.Conv2d(1, 32, kernel_size=3, padding=1)\n        self.conv2 = nn.Conv2d(32, 64, kernel_size=3, padding=1)\n        self.pool = nn.MaxPool2d(kernel_size=2, stride=2)\n        self.fc1 = nn.Linear(64 * 7 * 7, 128)\n        self.fc2 = nn.Linear(128, 26)\n        self.relu = nn.ReLU()\n        self.dropout = nn.Dropout(0.5)\n    \n    def forward(self, x):\n        x = self.pool(self.relu(self.conv1(x)))\n        x = self.pool(self.relu(self.conv2(x)))\n        x = x.view(-1, 64 * 7 * 7)  # 展平\n        x = self.relu(self.fc1(x))\n        x = self.dropout(x)\n        x = self.fc2(x)\n        return x\n\nmodel = CNN()\n\n# 定义损失函数和优化器，并添加权重衰减\ncriterion = nn.CrossEntropyLoss()\noptimizer = optim.Adam(model.parameters(), lr=0.001, weight_decay=1e-6)\n","metadata":{"execution":{"iopub.status.busy":"2024-07-18T08:12:11.383102Z","iopub.execute_input":"2024-07-18T08:12:11.383396Z","iopub.status.idle":"2024-07-18T08:12:11.400135Z","shell.execute_reply.started":"2024-07-18T08:12:11.383370Z","shell.execute_reply":"2024-07-18T08:12:11.39